package exerciceString;

public class User {
	
    int id;
    String lastName;
    String login;

    public User (int id, String lastName, String login) {
		this.id = id;
		this.lastName = lastName;
		this.login = login;
	}
    /**
     *  constructeur User 
     * 
     */
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getLastName() {
		return lastName;
	}

	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	public String getLogin() {
		return login;
	}

	public void setLogin(String login) {
		this.login = login;
	}

	
	/**
	 * j'ai modifier le toString pour mon besoin d'appel
	 * 
	 */
	public String toString() {
		return id + "," + lastName + "," + login;
	}
	
	
	
	
	
	

}
